{
  "name": "@saucelabs/nightwatch-sauce-visual-service",
  "description": "Nightwatch service that add visual checks capabilities using Sauce Labs Visual",
  "version": "0.5.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^16.13 || >=18"
  },
  "files": [
    "build"
  ],
  "typeScriptVersion": "3.8.3",
  "keywords": [
    "seleniumjs",
    "nightwatch",
    "wdio-service",
    "saucelabs",
    "visual"
  ],
  "dependencies": {
    "@saucelabs/visual": "^0.6.1",
    "chalk": "^4.1.2",
    "nightwatch": "^3.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "jest-junit-reporter": "^1.1.0",
    "prettier": "^2.8.8",
    "tsup": "^8.0.2"
  },
  "scripts": {
    "lint": "eslint \"{nightwatch,utils}/**/*.js\"",
    "build": "tsc --noEmit && tsup src",
    "test-ignored": "jest --collect-coverage"
  },
  "tsup": {
    "dts": "src/index.ts",
    "outDir": "./build",
    "format": [
      "cjs"
    ],
    "noExternal": [
      "@apollo/client"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
